3.
A.
#include <stdio.h>

int main() {

    char key, chose;
    int x, num, count;
    long date;
    float yield;
    double price;



    printf("%p \n", &key);
    printf("%p \n", &chose);
    printf("%p \n", &x);
    printf("%p \n", &num);
    printf("%p \n", &count);
    printf("%p \n", &date);
    printf("%p \n", &yield);
    printf("%p \n", &price);





    return 0;


}

B.
#include <stdio.h>

int main() {

    char key, chose;
    int x, num, count;
    long date;
    float yield;
    double price;



    printf("%p \n", &key);
    printf("%p \n", &chose);
    printf("%p \n", &x);
    printf("%p \n", &num);
    printf("%p \n", &count);
    printf("%p \n", &date);
    printf("%p \n", &yield);
    printf("%p \n", &price);


    printf("quantidade de bytes ocupada por char %ld \n",sizeof(char));
    printf("quantidade pcupada por int %ld \n",sizeof(int));
    printf("quantidade ocupada por long %ld \n",sizeof(long));
    printf("quantidade ocupada por float %ld \n",sizeof(float));
    printf("quantidade ocupada por price %ld \n",sizeof(double));





    return 0;


}




